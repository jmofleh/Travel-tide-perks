flight_fanatic_index\",\"bundle_index\"]]\n","    .drop_duplicates(\"user_id\")\n",")\n","\n","df_user_base = df_user_base.merge(indices, on=\"user_id\", how=\"left\")\n"],"metadata":{"id":"cJoDxykyq5Vl","executionInfo":{"status":"ok","timestamp":1770208867293,"user_tz":-60,"elapsed":72,"user":{"displayName":"Jawad Mofleh","userId":"03348460485281220387"}}},"execution_count":58,"outputs":[]},{"cell_type":"code","metadata":{"colab":{"base_uri":"https://localhost:8080/","height":1000},"id":"b44a5e72","executionInfo":{"status":"ok","timestamp":1770209187548,"user_tz":-60,"elapsed":332,"user":{"displayName":"Jawad Mofleh","userId":"03348460485281220387"}},"outputId":"43e8e307-ecdb-4979-e837-d03412f18b81"},"source":["# save dataframe\n","df_user_base.to_csv(f'{directory}/user_base.csv', index=False)\n","df_user_base.describe().T"],"execution_count":60,"outputs":[{"output_type":"execute_result","data":{"text/plain":["                             count           mean           std           min  \\\n","user_id                     5442.0  544646.575891  63475.654336  23557.000000   \n","num_sessions                5442.0       2.846196      1.356522      1.000000   \n","num_empty_sessions          5442.0       0.000000      0.000000      0.000000   \n","num_clicks                  5442.0      68.276369     37.806660      8.000000   \n","avg_clicks_per_session      5442.0      23.799990      7.248277      8.000000   \n","avg_session_duration        5442.0       2.948955      0.895584      0.966667   \n","std_session_duration        5442.0       0.836646      0.841699      0.000000   \n","num_canceled_trips           495.0       1.028283      0.165947      1.000000   \n","num_not_canceled_trips      5442.0       2.846196      1.356522      1.000000   \n","num_flights                 5442.0       4.729511      2.784809      0.000000   \n","num_hotels                  5442.0       2.553473      1.317250      0.000000   \n","avg_bags                    5442.0       0.536390     