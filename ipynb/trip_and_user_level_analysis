  else:\n","    return 'fall'\n","\n","\n","df_trips['season'] = df_trips.apply(season_helper, axis=1)\n","\n","df_trips[['user_id', 'season', 'family_and_friends', 'trip_id']].head()"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":206},"id":"36GVA8eh-5r2","executionInfo":{"status":"ok","timestamp":1770207588376,"user_tz":-60,"elapsed":180,"user":{"displayName":"Jawad Mofleh","userId":"03348460485281220387"}},"outputId":"48f62ecd-c204-4932-821a-ce6cb41c2638"},"execution_count":25,"outputs":[{"output_type":"execute_result","data":{"text/plain":["   user_id  season  family_and_friends  \\\n","0   512201  winter                   0   \n","1   517491  winter                   0   \n","2   488865  winter                   0   \n","3   560978  spring                   1   \n","4   542012  spring                   0   \n","\n","                                   trip_id  \n","0  512201-d198cb30dbe945a58808812558288792  \n","1  517491-f17c9a923a054db2955c3f56a7d8486e  \n","2  488865-1154619ac9b34950b501ead668e4e825  \n","3  560978-2d58a08a54cd4be1868835db447692f7  \n","4  542012-e368760a3dd34190ac40531d5383ec19  "],"text/html":["\n","  <div id=\"df-b84e4142-8980-459b-941b-a21ca8bab704\" class=\"colab-df-container\">\n","    <div>\n","<style scoped>\n","    .dataframe tbody tr th:only-of-type {\n","        vertical-align: middle;\n","    }\n","\n","    .dataframe tbody tr th {\n","        vertical-align: top;\n","    }\n","\n","    .dataframe thead th {\n","        text-align: right;\n","    }\n","</style>\n","<table border=\"1\" class=\"dataframe\">\n","  <thead>\n","    <tr style=\"text-align: right;\">\n","      <th></th>\n","      <th>user_id</th>\n","      <th>season</th>\n","      <th>family_and_friends</th>\n","      <th>trip_id</th>\n","    </tr>\n","  </thead>\n","  <tbody>\n","    <tr>\n","      <th>0</th>\n","      <td>512201</td>\n","      <td>winter</td>\n","      <td>0</td>\n","      <td>512201-d198cb30dbe945a58808812558288792</td>\n","    